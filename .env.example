# Database connection settings
# Copy this file to .env and modify as needed

# Database type: mysql or postgres
DB_TYPE=mysql

# Database host
DB_HOST=localhost

# Database port (default: 3306 for MySQL, 5432 for PostgreSQL)
DB_PORT=3306

# Database name
DB_NAME=testdb

# Database user
DB_USER=testuser

# Database password
DB_PASSWORD=testpass
